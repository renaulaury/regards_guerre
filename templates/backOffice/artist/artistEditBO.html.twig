{% extends 'base.html.twig' %}

{% block title %}Modification de l'artiste{% endblock %}

{% block body %}
<div class="containH1">    
    <h1>Modification de  {{ artist }}</h1>
</div>

<section class="#"> 
    
    {{ form_start(form) }}
        {{ form_row(form.artistName) }}
        {{ form_row(form.artistFirstname) }}
        {{ form_row(form.artistBirthDate) }}
        {{ form_row(form.artistDeathDate) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}

</section>
{% endblock %}


